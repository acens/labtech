<body>
<div class="page-loader"></div>
<!--========== SLIDER ==========-->
    <section id="home">
        <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
            <div class="container">
                <!-- Indicators -->
                <ol class="carousel-indicators">
                    <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
                    <li data-target="#carousel-example-generic" data-slide-to="1"></li>
                </ol>
            </div>

            <!-- Wrapper for slides -->
            <div class="carousel-inner" role="listbox">
                <div class="item active">
                    <img class="img-responsive" src="images/ilustration.jpg" alt="Slider Image">
                    <div class="container">
                        <div class="carousel-centered">
                            <div class="margin-b-40">
                                <h1 class="carousel-title">LAB TECH EXEMPLE 1</h1>
                                <p class="description">Em probabilidade e estatística, a distribuição normal é uma das
                                    <br/>distribuições de probabilidade mais utilizada para modelar fenômenos naturais.
                                </p>
                            </div>
                            <a href="#" class="btn-theme btn-theme-sm btn-white-brd text-uppercase">Saiba mais</a>
                        </div>
                    </div>
                </div>
                <div class="item">
                    <img class="img-responsive" src="images/ilustration4.jpg" alt="Slider Image">
                    <div class="container">
                        <div class="carousel-centered">
                            <div class="margin-b-40">
                                <h2 class="carousel-title">Lab Tech EXEMPLE 2</h2>
                                <p class="description">Em probabilidade e estatística, a distribuição normal é uma das
                                    <br/>distribuições de probabilidade mais utilizada para modelar fenômenos naturais.
                                </p>
                            </div>
                            <a href="#" class="btn-theme btn-theme-sm btn-white-brd text-uppercase">Saiba mais</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
</section>
<!--========== END SLIDER ==========-->


<!--========== PAGE LAYOUT BLOG ==========-->
    <section id="blog">
        <!-- Our Exceptional Solutions -->
        <div class="content-lg container">
            <div class="row margin-b-40">
                <div class="col-sm-6">
                    <h2>Últimas Notícias</h2>
                    <p class="description-blog">Acompanhe aqui as últimas notícias relacionadas à tecnologias úteis para auxilar seu negócio, assuntos referentes à computação e demais notícias sobre a LabTech.</p>
                </div>
            </div>
            <!--// end row -->

            <div class="row margin-b-50">
                <!-- Our Exceptional Solutions -->
                <% @post.each do |post|%>
                    <div class="col-md-4 col-sm-12 sm-margin-b-50">
                            <div class="margin-b-20">
                                <div class="margin-b-20">
                                    <div class="wow zoomIn" data-wow-duration=".3" data-wow-delay=".1s">
                                        <%= image_tag post.photo(:medium), class: "img-responsive"%>
                                    </div>
                                </div>
                            </div>
                            <h3><%= post.title%></h3>
                            <p class="description-blog"><%= post.description%></p>
                            <%= link_to "Leia mais", post_path(post), class: "link"%>
                            <!-- <a class="link" href="#">Leia mais</a> -->
                    </div>
                <%end%>
                <!-- End Our Exceptional Solutions -->
          </div>
            <!--// end BLOG -->
        </section>
<!--========== END PAGE LAYOUT BLOG ==========-->

<!--========== COURSE ==========-->
  <section id="course">
    <!-- Pricing -->
    <div class="bg-color-sky-light">
        <div class="content-lg container">
            <div class="row row-space-1">
                <div class="col-sm-4 sm-margin-b-2">
                    <div class="wow fadeInLeft" data-wow-duration=".3" data-wow-delay=".1s">
                        <!-- Pricing -->
                        <div class="pricing">
                            <div class="margin-b-30">
                                <i class="pricing-icon icon-chemistry"></i>
                                <h3>Curso de Big Data</h3>
                                <p class="description-blog">Descrição do curso</p>
                            </div>
                            <ul class="list-unstyled pricing-list margin-b-50">
                                <li class="pricing-list-item">lista 1</li>
                                <li class="pricing-list-item">lista 2</li>
                                <li class="pricing-list-item">lista 3</li>
                            </ul>
                            <%= link_to 'Inscrição' , subscriptions_url , class:  "btn-theme btn-theme-sm btn-default-bg text-uppercase" %>
                        </div>
                        <!-- End Pricing -->
                    </div>
                </div>
                <div class="col-sm-4 sm-margin-b-2">
                    <div class="wow fadeInUp" data-wow-duration=".3" data-wow-delay=".2s">
                        <!-- Pricing -->
                        <div class="pricing pricing-active">
                            <div class="margin-b-30">
                                <i class="pricing-icon icon-chemistry"></i>
                                <h3>Curso de Big Data</h3>
                                <p class="description-blog">Descrição do curso</p>
                            </div>
                            <ul class="list-unstyled pricing-list margin-b-50">
                                <li class="pricing-list-item">lista 1</li>
                                <li class="pricing-list-item">lista 2</li>
                                <li class="pricing-list-item">lista 3</li>
                            </ul>
                            <%= link_to 'Inscrição' , subscriptions_url , class:  "btn-theme btn-theme-sm btn-default-bg text-uppercase" %>
                        </div>
                        <!-- End Pricing -->
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="wow fadeInRight" data-wow-duration=".3" data-wow-delay=".1s">
                        <!-- Pricing -->
                        <div class="pricing">
                            <div class="margin-b-30">
                                <i class="pricing-icon icon-chemistry"></i>
                                <h3>Curso de Big Data</h3>
                                <p class="description-blog">Descrição do curso</p>
                            </div>
                            <ul class="list-unstyled pricing-list margin-b-50">
                                <li class="pricing-list-item">lista 1</li>
                                <li class="pricing-list-item">lista 2</li>
                                <li class="pricing-list-item">lista 3</li>
                            </ul>
                            <%= link_to 'Inscrição' , subscriptions_url , class:  "btn-theme btn-theme-sm btn-default-bg text-uppercase" %>
                        </div>
                        <!-- End Pricing -->
                    </div>
                </div>
            </div>
            <!--// end row -->
        </div>
    </div>
  </section>
<!--========== END COURSE ==========-->

<!--========== FOOTER / CONTACT ==========-->
  <section id="contact">
    <footer class="footer">
            <!-- Links -->
            <div class="footer-seperator">
                <div class="content-lg container">
                    <div class="row">
                        <div class="col-sm-5 sm-margin-b-30">
                            <h2 class="color-white">Entre em contato Conosco</h2>
                            <input type="text" class="form-control footer-input margin-b-20" placeholder="Name" required>
                            <input type="email" class="form-control footer-input margin-b-20" placeholder="Email" required>
                            <input type="text" class="form-control footer-input margin-b-20" placeholder="Phone" required>
                            <textarea class="form-control footer-input margin-b-30" rows="6" placeholder="Message" required></textarea>
                            <button type="submit" class="btn-theme btn-theme-sm btn-base-bg text-uppercase">Enviar</button>
                        </div>
                        <!-- Google Map -->
                        <div id="map" class="col-sm-7">
                          <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3981.1116287323575!2d-38.554970849531145!3d-3.785878397232303!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x7c74e93e3c021f9%3A0xc7981ffb851ce363!2sMACC!5e0!3m2!1spt-BR!2sbr!4v1516351781889" width="100%" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
                        </div>
                    </div>
                    <!--// end row -->
                </div>
            </div>
            <!-- End Links -->

            <!-- End Google Map -->

            <!-- Copyright -->
            <div class="content container">
                <div class="row">
                    <div class="col-xs-6">
                        <img class="footer-logo" src="images/LabTech_logo_dark.png" alt="Logo">
                    </div>
                    <div class="col-xs-6 text-right">
                        <p class="margin-b-0"><a class="color-base fweight-700" href="">Lab Tech</a>
                          site desenvolvido por <a class="color-base fweight-700" href="http://www.acensjr.com/" target="_blank" >Acens</a>
                        </p>
                    </div>
                </div>
                <!--// end row -->
            </div>
            <!-- End Copyright -->
        </footer>
    </section>
<!--========== END FOOTER / CONTACT ==========-->

